<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.hello-world"
        version   = "2.0.0">
    <name>Watcher</name>

    <description>
        Aplicacion para registro de reportes de prefectura.
    </description>

    <author href="http://" email="fjcoutop@gmail.com">
        Francisco Jose Couto Pelaez
    </author>

    <feature name="http://api.phonegap.com/1.0/device" />

    <preference name="phonegap-version" value="2.1.0" />
    <preference name="orientation"      value="portrait" />
    <preference name="target-device"    value="universal" />
    <preference name="fullscreen"       value="true" />

    <icon src="icon.png" />
    <icon src="res/icon/android/icon-48.png"   gap:platform="android"    gap:density="mdpi" />
    
    
    <gap:splash src="res/screen/android/screen-mdpi-portrait.png"  gap:platform="android" gap:density="mdpi" />
    
	
	<access origin="https://build.phonegap.com" />
	
</widget>